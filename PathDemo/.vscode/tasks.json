{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/EHsc", "/nologo",
                "/O2", "/arch:AVX512",
                "/I", "sdl\\include",
                "/Fe:",
                "demo.exe",
                "${workspaceFolder}\\*.cpp",
                "${workspaceFolder}\\sdl\\lib\\x64\\SDL2.lib",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        },
    ],
    "version": "2.0.0"
}