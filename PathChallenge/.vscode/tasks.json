{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/EHsc", "/nologo",
                "/O2", "/arch:AVX2",
                "/Fe:", "pathchallenge.exe",
                "${workspaceFolder}\\*.cpp",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        },
        {
            "label": "run main",
            "type": "shell",
            "command": ".\\pathchallenge.exe",
            "dependsOn": "C/C++: cl.exe build active file"
        }
    ],
    "version": "2.0.0"
}